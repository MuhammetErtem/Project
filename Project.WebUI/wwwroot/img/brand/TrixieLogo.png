if($('#live-search').val() == ''){
$('#live-search-box').css({
'visibility': 'hidden'
});
}
else{
interval = setInterval(function(){
if($('#live-search').val() != keyHistory){
$('#live-search-box').attr('data-url', '/srv/service/product/searchAll/' + $('#live-search').val()).angularTemplate();
keyHistory = $('#live-search').val();
}
}, 1000);
}
});
